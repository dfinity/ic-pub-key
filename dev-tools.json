{
  "rustup": {
    "method": "curl",
    "version": "1.28.1",
    "url": "https://raw.githubusercontent.com/rust-lang/rustup/${VERSION}/rustup-init.sh",
    "pipe": [["sh"]]
  },
  "rust": {
    "method": "sh",
    "note": "See also: rust-toolchain.toml",
    "version": "nightly-2025-04-01"
  },
  "shellcheck": {
    "method": "apt"
  },
  "shfmt": {
    "method": "go",
    "version": "v3.5.1",
    "source": "mvdan.cc/sh/v3/cmd/shfmt"
  },
  "cargo-binstall": {
    "method": "sh",
    "version": "1.7.4"
  },
  "cargo-audit": {
    "method": "cargo-binstall",
    "version": "0.21.2"
  },
  "cargo-edit": {
    "method": "cargo-binstall",
    "version": "0.13.0"
  },
  "cargo-sort": {
    "method": "cargo-binstall",
    "version": "1.0.9"
  },
  "zizmor": {
    "method": "cargo-binstall",
    "version": "1.5.2"
  }
}
